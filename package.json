{
  "name": "thaco",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-app-rewired build",
    "start": "react-app-rewired start",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "lint:css": "stylelint src/**/*.scss --fix",
    "lint:js": "eslint src/**/*.{ts,tsx} --fix",
    "dk:build": "docker build ./ -t frontend-a",
    "dk:build-sb": "docker build ./ -f storybook.Dockerfile -t frontend-c",
    "dk:up": "docker run -it --rm -p 80:80 frontend-a",
    "dk:up-sb": "docker run -it --rm -p 8888:80 frontend-c",
    "dk:up-fg": "docker run -d -it --rm -p 80:80 frontend-a"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config ./commitlint.config.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --ext .ts,.tsx --fix",
    "*.scss": "stylelint --fix"
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 edge version",
      "last 1 safari version",
      "ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 edge version",
      "last 1 safari version",
      "ie 11"
    ]
  },
